{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/dashboard",
        "destination": "/dashboard.html"
      },
      {
        "source": "/siswa",
        "destination": "/siswa.html"
      },
      {
        "source": "/bilqolam",
        "destination": "/bilqolam.html"
      },
      {
        "source": "/doa",
        "destination": "/doa.html"
      },
      {
        "source": "/tathbiq",
        "destination": "/tathbiq.html"
      }
    ]
  }
}